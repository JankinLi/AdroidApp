<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.lichuan.test01"
    android:versionCode="1"
    android:versionName="1.0" >
<!--
    <uses-sdk
        android:minSdkVersion="23"
        android:targetSdkVersion="28" />
-->
    <application
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.lichuan.test01.MainActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name="com.lichuan.test01.service.Loading" />

        <activity
            android:name="com.lichuan.test01.AddClothActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_add_cloth" >
        </activity>
        <activity
            android:name="com.lichuan.test01.PicViewActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_pic_view" >
        </activity>
        <activity
            android:name="com.lichuan.test01.ClothListActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_cloth_list" >
        </activity>
        <activity
            android:name="com.lichuan.test01.AddBoxActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_add_box" >
        </activity>
        <activity
            android:name="com.lichuan.test01.AddSuitActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_add_suit" >
        </activity>
    </application>

    <uses-permission android:name="android.permission.READ_CONTACTS"
        tools:ignore="ManifestOrder" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
</manifest>
